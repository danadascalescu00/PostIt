<p>
ID: <%= user.id %><br/>
Username: <%= user.username %><br/>
Name: <%= user.displayName %><br/>
<% if (user.emails) { %>
Email: <%= user.emails[0].value %><br/>
<% } %>
<%= user.pageToken %>
</p>
<% if (facebook_user) { %>
	<p>Hello, <%= facebook_user.displayName %> </p>
<% } else { %>
	<a href="/login/facebook">Log In with Facebook</a>
<% } %>

<a href="/logout">Log out</a>